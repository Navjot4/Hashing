#include<bits/stdc++.h>
using namespace std;
unordered_map<int,int> freq;
void freq(int a[],int n)
{
   for(int i=0;i<n;i++)
   {
      freq[a[i]]++;
    }
  }
int freq(int x)
{
   return freq[x];
}


    int main()
{
    int a[] = {1, 3, 2, 4, 2, 1};
    int n = sizeof(a)/sizeof(a[0]);
    countFreq(a, n);
    cout << query(2) << endl;
    cout << query(3) << endl;
    cout << query(5) << endl;
    return 0;
}
